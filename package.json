{
  "name": "discord-bot-base",
  "version": "1.0.0",
  "description": "A discord bot base",
  "private": true,
  "type": "module",
  "main": "build/client.js",
  "scripts": {
    "dev": "node --loader ts-node/esm/transpile-only src/client.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/client.ts",
    "build": "tsc",
    "build:serve": "npm run build && node build/client.js",
    "serve": "node build/client.js",
    "lint": "eslint",
    "format": "prettier --write ."
  },
  "author": "SebHex",
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/utilities": "^3.1.5",
    "discord.js": "^13.3.1",
    "discordx": "^9.0.1",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  }
}
